
.socials{
  display: flex;
  align-items: center;

&__icon {
  width: 28px;
  height: 31px;
  display: block;
  fill: #716c68;
}

&__item{
  margin-right: 40px;

  &:last-child{
    margin-right: 0;
  } 
}

&__item-footer{
  margin-right: 40px;

  &:last-child{
    margin-right: 0;
  } 
}

&__icon-footer{
  width: 20px;
  height: 30px;
  display: block;
  fill: #716c68;
}

}
